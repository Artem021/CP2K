 &GLOBAL
   PRINT_LEVEL MEDIUM
   PROJECT atropo
   RUN_TYPE MD
   SEED 1642
 &END GLOBAL
 &FORCE_EVAL
   &EXTERNAL_POTENTIAL
     ATOMS_LIST 1..118
     FUNCTION 0.0000000000001*(Z^2)^4
   &END EXTERNAL_POTENTIAL
   &EXTERNAL_POTENTIAL
     ATOMS_LIST 1..118
     FUNCTION 0.0000000000001*(X^2)^4
   &END EXTERNAL_POTENTIAL
   &EXTERNAL_POTENTIAL
     ATOMS_LIST 1..118
     FUNCTION 0.0000000000001*(Y^2)^4
   &END EXTERNAL_POTENTIAL
   METHOD FIST
   &MM
     &FORCEFIELD
       ALL_EI_SCALE 1.0
       ALL_VDW_SCALE 1.0
       EI_SCALE14 1.0
       FORCE_SCALE 0.0
       PARMTYPE AMBER
       PARM_FILE_NAME atropoisomer.prmtop
       SHIFT_CUTOFF F
       &SPLINE
         EMAX_SPLINE 1.0E+08
         RCUT_NB [angstrom] 1.0E+01
       &END SPLINE
     &END FORCEFIELD
     &POISSON
       &EWALD
         ALPHA .40
         EWALD_TYPE NONE
         GMAX 80
       &END EWALD
       PERIODIC NONE
     &END POISSON
   &END MM
   &RESCALE_FORCES
     MAX_FORCE 10
   &END RESCALE_FORCES
   STRESS_TENSOR ANALYTICAL
   &SUBSYS
     &CELL
       ABC [angstrom] 100 100 100
       ALPHA_BETA_GAMMA 90 90 90
       PERIODIC NONE
     &END CELL
     &TOPOLOGY
       &CENTER_COORDINATES
         CENTER_POINT 0 0 0
       &END CENTER_COORDINATES
       CONN_FILE_FORMAT AMBER
       CONN_FILE_NAME atropoisomer.prmtop
       COORD_FILE_FORMAT XYZ
       COORD_FILE_NAME atropoisomer-1.xyz
     &END TOPOLOGY
   &END SUBSYS
 &END FORCE_EVAL
 &MOTION
   &MD
     ENSEMBLE NVT
     STEPS 1000
     TEMPERATURE 298
     &THERMOSTAT
       &CSVR
         TIMECON [fs] 0.5
       &END CSVR
       REGION GLOBAL
       TYPE CSVR
     &END THERMOSTAT
     TIMESTEP [fs] 2.0
   &END MD
   &PRINT
     &RESTART
       BACKUP_COPIES 0
       &EACH
         MD 0
       &END EACH
     &END RESTART
     &RESTART_HISTORY
       &EACH
         MD 0
       &END EACH
     &END RESTART_HISTORY
     &TRAJECTORY
       &EACH
         MD 5
       &END EACH
       FORMAT XYZ
     &END TRAJECTORY
   &END PRINT
 &END MOTION
